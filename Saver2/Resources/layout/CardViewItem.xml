
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding = "4dp"
    android:background="#33414E">
    <android.support.v7.widget.CardView
      app:cardBackgroundColor = "@android:color/white"
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/cardViewItems1"
        app:cardCornerRadius="8dp">
      <LinearLayout
            android:orientation="vertical"
            android:minWidth="0px"
            android:minHeight="0px"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/linearLayout1">
            <LinearLayout
                android:paddingHorizontal = "4dp"
                android:paddingVertical = "1dp"
                android:orientation="vertical"
                android:minWidth="0px"
                android:minHeight="0px"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/linearLayout11">
              <TextView
                android:maxLines = "1"
                android:textStyle = "bold"
                android:layout_alignBaseline="@+id/textViewIcon3"
                android:layout_toEndOf="@+id/txtvKodas_ps"
                android:layout_alignParentRight="true"
                android:textSize = "12sp"
                android:textColor = "@android:color/black"
                android:text=""
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/txtvPav" />
            </LinearLayout>

        <RelativeLayout
                android:paddingHorizontal = "4dp"
                android:orientation="horizontal"
                android:minWidth="0px"
                android:minHeight="0px"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/linearLayout9">
                <TextView
                  android:layout_alignParentLeft="true"
                  android:textSize = "20sp"
                  android:text = ""
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:id="@+id/textViewIcon1" />
                <TextView
                  android:paddingLeft = "4dp"
                  android:layout_alignBaseline="@+id/textViewIcon1"
                  android:layout_toEndOf="@+id/textViewIcon1"
                  android:textSize = "14sp"
                  android:textColor = "@android:color/holo_red_dark"
                  android:text="                           "
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:id="@+id/txtvKodas_lc_a_ls" />
                <TextView
                  android:textSize = "20sp"
                  android:layout_alignBaseline="@+id/textViewIcon1"
                  android:layout_toStartOf="@+id/txtvKiekis"
                  android:text=""
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:id="@+id/textViewIcon2" />
                <TextView
                  android:textSize = "14sp"
                  android:paddingLeft = "4dp"
                  android:layout_alignBaseline="@+id/textViewIcon1"
                  android:layout_alignParentRight="true"
                  android:textColor = "@android:color/black"
                  android:text="    "
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:id="@+id/txtvKiekis" />
            </RelativeLayout>
            
            <RelativeLayout
                android:paddingHorizontal = "4dp"
                android:paddingVertical = "1dp"
                android:gravity="bottom"
                android:orientation="horizontal"
                android:minWidth="0px"
                android:minHeight="0px"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/linearLayout10">
                  <TextView
                    android:layout_alignParentLeft="true"
                    android:text=""
                    android:textSize = "20sp"
                    android:textColor = "@android:color/black"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textViewIcon3" />
                  <TextView
                    android:paddingLeft = "4dp"
                    android:layout_toEndOf="@+id/textViewIcon3"
                    android:layout_alignBaseline="@+id/textViewIcon3"
                    android:textSize = "12sp"
                    android:textColor = "#4889A1"
                    android:text=""
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/txtvKodas_ps" />

                      <TextView
                        android:layout_alignBaseline="@+id/textViewIcon3"
                        android:layout_alignWithParentIfMissing="true"
                        android:layout_toLeftOf="@+id/txtvKodas_os"
                        android:background = "@drawable/bg_orange"
                        android:textSize = "10sp"
                        android:textColor = "@android:color/white"
                        android:text="ZZZ"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/txtvSerija" />
               <TextView
                  android:layout_alignBaseline="@+id/textViewIcon3"
                  android:layout_alignParentRight="true"
                  android:background = "@drawable/bg_blue"
                  android:textSize = "10sp"
                  android:textColor = "@android:color/white"
                  android:text="DDD"
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:id="@+id/txtvKodas_os" />
            </RelativeLayout>
        </LinearLayout>
    </android.support.v7.widget.CardView>
</LinearLayout>